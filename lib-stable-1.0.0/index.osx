const ENV = process.env.NODE_ENV;
if (ENV !== 'production' &&
    ENV !== 'test' &&
    typeof console !== 'undefined' &&
    console.warn &&
    typeof window !== 'undefined') {
    console.warn(
        '您采用了加载全部组件的方式开发, ' +
        '为了减少减少网络延迟，建议使用"按需加载"： https://www.npmjs.com/package/babel-plugin-import 方式。',
    );
}